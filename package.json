{
  "name": "oembed-spec",
  "description": "A parser for oEmbed specification",
  "homepage": "https://nicedoc.io/microlinkhq/oembed-spec",
  "version": "1.3.2",
  "main": "src/index.js",
  "author": {
    "email": "hello@microlink.io",
    "name": "microlink.io",
    "url": "https://microlink.io"
  },
  "contributors": [
    {
      "name": "Kiko Beats",
      "email": "josefrancisco.verdu@gmail.com"
    },
    {
      "name": "RÃ©mi Berson",
      "email": "remi@cliqz.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlinkhq/oembed-spec.git"
  },
  "bugs": {
    "url": "https://github.com/microlinkhq/oembed-spec/issues"
  },
  "keywords": [
    "oembed",
    "parser",
    "spec"
  ],
  "dependencies": {
    "got": "~10.7.0",
    "tldts": "~5.6.19"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@microlink/demo-links": "latest",
    "ava": "latest",
    "benchmark": "latest",
    "big-list-of-naughty-strings": "latest",
    "chalk": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "coveralls": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "git-dirty": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "nanobench": "latest",
    "npm-check-updates": "latest",
    "nyc": "latest",
    "oembed-parser": "latest",
    "prettier-standard": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 10"
  },
  "files": [
    "scripts",
    "src"
  ],
  "scripts": {
    "benchmark": "node ./benchmarks/benchmark.js",
    "clean": "rm -rf node_modules",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "standard-markdown README.md && standard",
    "postinstall": "node scripts/postinstall.js",
    "postrelease": "npm run release:tags && npm run release:github && ci-publish",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "nyc ava test/index.js test/find-provider/*.js test/providers/*.js",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "finepack"
    ],
    "*.js": [
      "prettier-standard"
    ],
    "*.md": [
      "standard-markdown"
    ]
  }
}
